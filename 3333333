#include<stdio.h>
int main()
{
    int start, end, i, sum = 0;
    printf("Enter the starting number:");
    scanf("%d", &start);

    printf("Enter the ending number:");
    scanf("%d", &end);

    for(int i = start+1; i < end; i++)
    {
        if(i % 2 != 0)
        {
            sum = sum + i;
        }
    }

    printf("The sum of odd number between %d and %d is : %d\n", start, end, sum);

    return 0;
}
